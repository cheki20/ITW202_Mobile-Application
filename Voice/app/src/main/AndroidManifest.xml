<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gcitcomplaint.voice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Voice">
        <activity
            android:name=".header"
            android:label="@string/title_activity_header"
            android:theme="@style/Theme.Voice.NoActionBar" />
        <activity android:name=".RegisterAdmin" />
        <activity android:name=".LogdeComplaint" />
        <activity android:name=".User_Profile" />
        <activity android:name=".AddServiceProvider" />
        <activity android:name=".AddUser" />
        <activity android:name=".ServiceProviderDashboard" />
        <activity android:name=".UserDashboard" />
        <activity android:name=".AdminDashboard" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>